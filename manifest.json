{
    "manifest_version": 2,
    "name": "One-click Pics",
    "description": "This extension demonstrates a browser action with pics.",
    "version": "0.1",
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ],
    "browser_action": {
        "default_icon": "icon.jpeg",
        "default_popup": "popup.html"
    }
}